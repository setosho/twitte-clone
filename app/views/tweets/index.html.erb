<h1>Tweetsの一覧(index)</h1>

<p><%= notice %></p>

<table>
  <tr>

    <th>内容</th>

  </tr>

<% @tweets.each do |tweet| %>
  <tr>

    <td><%= tweet.content %></td>
    <td><%= link_to "ツイートを編集する", edit_tweet_path(tweet.id), data: {confirm: '本当に編集しますか？'} %></td>
    <td><%= link_to 'ツイートを削除する', tweet_path(tweet.id), method: :delete ,data: { confirm:'本当に削除しますか？'} %></td>
  </tr>
<% end %>
</table>

<br>
<%= link_to '新しくツイートを投稿する', new_tweet_path %>
